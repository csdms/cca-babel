#
# Run the tests
#

echo "TEST_BEGIN 1 Ordering.sidl->sidl"
echo "PART 1"
diff ./sidl/Ordering.sidl ./xml/Ordering.sidl > /dev/null \
     && (echo "RESULT 1 PASS" && echo "TEST_RESULT PASS") \
     || (echo "RESULT 1 FAIL" && echo "TEST_RESULT FAIL") 

echo
echo "TEST_BEGIN 1 Ordering.IntOrderTest.XML->XML"
echo "PART 1"
grep -v Metadata ./xml/Ordering.IntOrderTest-*.xml > temp
grep -v Metadata ./xml2/Ordering.IntOrderTest-*.xml > temp2
diff temp temp2 > /dev/null \
     && (echo "RESULT 1 PASS" && echo "TEST_RESULT PASS") \
     || (echo "RESULT 1 FAIL" && echo "TEST_RESULT FAIL") 
echo
echo "TEST_BEGIN 1 Ordering.XML->XML"
echo "PART 1"
grep -v Metadata ./xml/Ordering-*.xml > temp
grep -v Metadata ./xml2/Ordering-*.xml > temp2
diff temp temp2 > /dev/null \
     && (echo "RESULT 1 PASS" && echo "TEST_RESULT PASS") \
     || (echo "RESULT 1 FAIL" && echo "TEST_RESULT FAIL") 

rm -rf temp temp2
